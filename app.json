{
  "expo": {
    "name": "auth_project",
    "slug": "auth_project",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo_favicon_icon.png",
    "userInterfaceStyle": "dark",
    
    "splash": {
      "image": "./assets/logo_favicon_icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#101010"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "userInterfaceStyle": "automatic",
      "bundleIdentifier": "authProject"
    },
    "android": {
      "userInterfaceStyle": "automatic",
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo_favicon_icon.png",
        "backgroundColor": "#101010"
      },
      "package": "com.samuel_chost.auth_project"
    },
    "web": {
      "favicon": "./assets/logo_favicon_icon.png"
    },
    "plugins": [
      "expo-secure-store",
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/LexendExa.ttf"
          ]
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "238111ce-6709-4c66-bdc5-cd9f712e63ed"
      }
    },
    "owner": "samuel_chost"
  }
}
